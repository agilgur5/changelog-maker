    "ghissues": "~1.1.3",
    "gitexec": "~1.0.0",
    "list-stream": "~1.0.1",
    "minimist": "~1.2.0",
    "pkg-to-id": "~0.0.3",
    "split2": "~3.0.0",
    "strip-ansi": "~5.0.0"
